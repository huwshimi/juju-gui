{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-react-jsx"
  ],
  "env": {
    "production": {
      "presets": [
        ["babili", {"mangle": false}]
      ]
    },
    "development": {
      "plugins": [
        "rewire"
      ]
  },
    "test": {
      "plugins": [
        "rewire"
      ],
      "presets": [
        "env",
        "react"
      ],
    }
  }
}
